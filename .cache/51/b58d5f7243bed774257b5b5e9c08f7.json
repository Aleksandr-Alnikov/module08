(i) => {\r\n    const size = window.matchMedia('(max-width: 1024px)');\r\n\r\n    if (i > slides.length) {\r\n        index = 1;\r\n    }\r\n    if (i < 1) {\r\n        index = slides.length;\r\n    }\r\n\r\n    const handleSize = (e) => {\r\n        slides.forEach(item => {\r\n            if (e.matches) {\r\n                item.style.display = 'none';\r\n                slides[index - 1].style.display = '';\r\n            } else {\r\n                item.style.display = '';\r\n            }\r\n        });\r\n    };\r\n\r\n    size.addEventListener('change', handleSize);\r\n    handleSize(size);\r\n\r\n    if (slides.length < 10) {\r\n        current.textContent = `0${index}`;\r\n    } else {\r\n        current.textContent = index;\r\n    }\r\n};\r\n\r\n\r\n export const launchSlide = () => {\r\n     const plusSlide = (i) => {\r\n        showSlide(index += i);\r\n    };\r\n\r\n    leftBtn.addEventListener('click', () => {\r\n        plusSlide(-1);\r\n    });\r\n\r\n    rightBtn.addEventListener('click', () => {\r\n        plusSlide(1);\r\n    });\r\n};\r\n\r\n\r\nshowSlide(index)\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"bdbe727be31e9a04cca2dec80fde3ac1","cacheData":{"env":{}}}