rger.css/burger.css","original":{"line":209,"column":0},"generated":{"line":209,"column":0}},{"source":"node_modules/burger.css/burger.css","original":{"line":210,"column":0},"generated":{"line":210,"column":0}},{"source":"node_modules/burger.css/burger.css","original":{"line":211,"column":0},"generated":{"line":211,"column":0}},{"source":"node_modules/burger.css/burger.css","original":{"line":212,"column":0},"generated":{"line":212,"column":0}}],"sources":{"node_modules/burger.css/burger.css":"html,\nbody {\n  margin: 0;\n  font-size: 3rem;\n}\n*,\n*::after,\n*::before {\n  box-sizing: border-box;\n}\n\n[data-burger] {\n  display: flex;\n  flex-direction: column;\n  transition: all 300ms ease-in-out;\n  /* gap: 1rem; */\n  /* justify-content: center; */\n  overflow: hidden;\n  width: 2.75em;\n}\n/* scale doesn't work yet for ingredients\n\n[data-burger~=\"size:small\"] {\n  width: 2.5em;\n}\n[data-burger~=\"size:medium\"] {\n}\n[data-burger~=\"size:large\"] {\n  width: 3em;\n} */\n[data-bun] {\n  --bun--outside-color: rgb(255, 155, 0);\n  --bun--inside-color: rgb(255, 219, 135);\n}\n[data-burger--wheat-bun] [data-bun],\n[data-wheat-bun] {\n  --bun--outside-color: rgb(189, 88, 0);\n  --bun--inside-color: rgb(224, 146, 79);\n}\n[data-bun~=\"crown\"] {\n  height: 0.6em;\n  background-image: linear-gradient(\n    var(--bun--outside-color),\n    var(--bun--inside-color)\n  );\n  border-radius: 1em 1em 4px 4px;\n}\n[data-burger--bun~=\"toasted\"] [data-bun~=\"crown\"],\n[data-bun~=\"crown\"][data-bun~=\"toasted\"] {\n  border-bottom: 4px solid brown;\n}\n[data-bun~=\"heel\"] {\n  height: 0.35em;\n  background-image: linear-gradient(\n    var(--bun--inside-color),\n    var(--bun--outside-color)\n  );\n  border-radius: 4px 4px 0.25em 0.25em;\n}\n[data-burger--bun~=\"toasted\"] [data-bun~=\"heel\"],\n[data-bun~=\"heel\"][data-bun~=\"toasted\"] {\n  border-top: 4px solid brown;\n}\n[data-patty] {\n  border-radius: 0.125em;\n  height: var(--patty--height);\n  background-image: linear-gradient(\n    to bottom,\n    rgb(98, 18, 0),\n    brown 25.63%,\n    rgb(157, 41, 35) 75.21%,\n    rgb(116, 29, 9)\n  );\n}\n\n[data-burger--patty=\"cook:medium-rare\"] [data-patty],\n[data-patty~=\"cook:medium-rare\"] {\n  border-radius: 0.125em;\n  height: 0.7rem;\n  background-image: linear-gradient(\n    to bottom,\n    rgb(98, 18, 0),\n    brown 15%,\n    rgb(157, 41, 35) 85%,\n    rgb(116, 29, 9)\n  );\n}\n[data-burger--patty=\"cook:medium\"] [data-patty],\n[data-patty~=\"cook:medium\"] {\n  border-radius: 0.125em;\n  height: var(--patty--height, 0.7rem);\n  background-image: linear-gradient(\n    to bottom,\n    rgb(98, 18, 0),\n    brown 30%,\n    rgb(157, 41, 35) 70%,\n    rgb(116, 29, 9)\n  );\n}\n[data-burger--patty=\"cook:medium-well\"] [data-patty],\n[data-patty~=\"cook:medium-well\"] {\n  border-radius: 0.125em;\n  height: 0.7rem;\n  background-image: linear-gradient(\n    to bottom,\n    rgb(98, 18, 0),\n    brown 50%,\n    rgb(157, 41, 35) 50%,\n    rgb(116, 29, 9)\n  );\n}\n[data-burger--patty~=\"weight:60g\"] [data-patty],\n[data-patty~=\"weight:60g\"] {\n  --patty--height: 0.6rem;\n}\n[data-burger--patty~=\"weight:70g\"] [data-patty],\n[data-patty~=\"weight:70g\"] {\n  --patty--height: 0.7rem;\n}\n[data-burger--patty~=\"weight:80g\"] [data-patty],\n[data-patty~=\"weight:80g\"] {\n  --patty--height: 0.8rem;\n}\n[data-burger--patty~=\"weight:90g\"] [data-patty],\n[data-patty~=\"weight:90g\"] {\n  --patty--height: 0.9rem;\n}\n[data-burger--patty~=\"weight:100g\"] [data-patty],\n[data-patty~=\"weight:100g\"] {\n  --patty--height: 1rem;\n}\n[data-cheese] {\n  --cheese--color: rgb(255, 230, 0);\n  background-color: var(--cheese--color);\n  border-radius: 0.5rem 0.5rem 0 0;\n  width: 2.4em;\n  align-self: center;\n  position: relative;\n  height: 0.2rem;\n}\n[data-cheese]::after {\n  content: \"\";\n  display: block;\n  width: 0.3em;\n  height: 0.3em;\n  background-image: linear-gradient(\n    45deg,\n    var(--cheese--color) 50%,\n    transparent 50%\n  );\n  transform: rotate(-45deg);\n  position: absolute;\n  right: 0.3em;\n  bottom: -0.15em;\n  /* background-color: inherit; */\n  /* border-radius: 100% 100% 0 0; */\n  /* border-color: inherit; */\n  /* border: 20px 0 20px 0 solid blue; */\n}\n[data-cheese] ~ [data-cheese]::after {\n  left: 0.5em;\n}\n[data-cheddar-cheese] {\n  --cheese--color: orange;\n}\n[data-jack-cheese] {\n  --cheese--color: whitesmoke;\n  border: 1px solid hsl(0, 0%, 80%);\n}\n[data-mustard] {\n  background-color: rgb(248, 255, 0);\n  height: 0.15rem;\n}\n[data-katchup] {\n  background-color: red;\n  height: 0.15rem;\n}\n[data-spread] {\n  background-color: red;\n  height: 0.15rem;\n}\n[data-pickles] {\n  background-image: repeating-linear-gradient(\n    to right,\n    transparent,\n    transparent 0.5rem,\n    green 0.4rem,\n    green 1.15em\n  );\n  width: 3em;\n  height: 0.2rem;\n}\n[data-pickles~=\"cut:thin\"] {\n  height: 0.1rem;\n}\n[data-pickles~=\"cut:thick\"] {\n  height: 0.3rem;\n}\n[data-tomato] {\n  background-color: tomato;\n  width: 2.4em;\n  height: 0.3rem;\n  align-self: center;\n  border-radius: 2px;\n}\n[data-tomato=\"cut:thin\"] {\n  height: 0.4rem;\n}\n[data-tomato=\"cut:thick\"] {\n  height: 0.5rem;\n}\n"},"lineCount":212}},"error":null,"hash":"1e4d38038b4fd11359cf83026d3b2da2","cacheData":{"env":{}}}